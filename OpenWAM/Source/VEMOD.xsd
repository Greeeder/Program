	<element name="HeatExchangers"> <!-- Intercambiadores de calor -->
        <complexType>
            <sequence>
                <element maxOccurs="unbounded" minOccurs="0" name="HeatExchanger"> <!-- Intercambiador de calor -->
                    <complexType>
						<attribute name="ID" type="unsignedInt"></attribute>
							<!-- Identificador del intercambiador de calor (respecto a otros intercambiadores del motor) -->
							<!-- valor por defecto: enumeración entre intercambiadores -->
							<!-- Alternativa preferible: no mostrar el campo al usuario e imponerle el valor por defecto directamente -->
						<attribute name="Name" type="string"></attribute> <!-- valor por defecto: "HT_x" con x = número de intercambiador -->
                        <attribute name="Fluid type">  <!-- Tipo de fluidos en el intercambiador -->
                            <simpleType>
                                <restriction base="string" use="required">
									<!-- El tipo de fluidos debe limitar las pantallas a las que el intercambiador puede ser copiado -->
									<!-- Ejemplo: un intercambiador GAS-GAS no puede ser copiado a una ventana de circuito hidráulico (de líquidos) -->
                                    <!-- Si el intercambiador ha sido creado en una pantalla incorrecta, mostrar advertencia -->
									<enumeration value="GAS-GAS"/>
                                    <enumeration value="GAS-LIQUID"/>
									<enumeration value="GAS-AMBIENT"/>
									<enumeration value="LIQUID-LIQUID"/>
									<enumeration value="LIQUID-AMBIENT"/>
                                </restriction>
							</simpleType>
                        </attribute>
						<attribute name="Fluid 1">
							<!-- De entre ambos fluidos, selecciona qué fluido es el 1 -->
							<!-- El fluido del circuito actual (agua, aceite, refrigerante, aire, gas de escape...) lo debe haber seleccionado en la pantalla general del circuito -->
							<!-- Cada fluido correspondera a un circuito diferente
							<!-- Se puede elegir entre el ambiente, el fluido del circuito actual, el del otro circuito (si es posible) o dejarlo en blanco para agregar más tarde el fluido de otro circuito (en la pantalla de otro circuito) -->
                            <!-- Cuando todo en el motor esté definido, este campo no puede quedar en blanco -->
							<simpleType>
                                <restriction base="string" use="required">
									<enumeration value="Current circuit fluid"/> <!-- Fluido del circuito actual -->
									<enumeration value="Fluid of circuit X"/> <!-- Fluido de otro circuito X (si es posible; si no, sólo se podría seleccionar el fluido de cada circuito en la pantalla de cada circuito) -->
                                    <!-- (... resto de circuitos ...) -->
									<enumeration value="Ambient"/> <!-- Ambiente (el ambiente a priori no tendrá una pantalla propia de circuito) -->
                                </restriction>
							</simpleType>
                        </attribute>
						<attribute name="Fluid 2"> <!-- Análogo a Fluid 1 -->
							<simpleType>
                                <restriction base="string" use="required">
									<enumeration value="Current circuit fluid"/> <!-- Fluido del circuito actual -->
									<enumeration value="Fluid of circuit X"/> <!-- Fluido de otro circuito X (si es posible; si no, sólo se podría seleccionar el fluido de cada circuito en la pantalla de cada circuito) -->
                                    <!-- (... resto de circuitos ...) -->
									<enumeration value="Ambient"/> <!-- Ambiente (el ambiente a priori no tendrá una pantalla propia de circuito) -->
                                </restriction>
							</simpleType>
                        </attribute>
						<attribute name="Volume" type="double" use="required"></attribute> <!-- Volumen [m^3] -->
						<attribute name="Hydraulic resistance" type="double" use="required"></attribute> <!-- Resistencia hidráulica al flujo de fluido [m.c.f * m^(-6) * s^2)] -->
						<attribute name="UA" type="double" use="required"></attribute> <!-- Coeficiente global de transmision de calor por el área de intercambio de calor (conductancia) [W/K] -->
						
						<!-- Cada intercambiador puede tener la eficiencia definida mediante una tabla o mediante las caracteristicas del itercambiador -->
						<!-- Hay que seleccionar una de las dos opciones -->
						<!-- Esto se puede hacer mediante una lista con dos opciones o con una casilla -->
						<!-- Lo pongo como lista porque no sé cómo hacer casillas -->
						<attribute name="Efficiency calculation method"> <!-- Método para el cálculo de la eficiencia -->
							<simpleType>
                                <restriction base="string" use="required">
									<enumeration value="From heat exchanger type"/> <!-- A partir del tipo de intercambiador de calor -->
									<enumeration value="From specified table"/> <!-- A partir de la tabla especificada -->
                                </restriction>
							</simpleType>
                        </attribute>
						
						<!-- Según el método elegido, al usuario se le permitirá editar una serie de campos o rellenar una tabla -->
						<!-- OPCIÓN TIPO DE INTERCAMBIADOR -->
						<attribute name="Heat exchanger type"> <!-- Tipo de intercambiador de calor -->
							<simpleType>
                                <restriction base="string" use="required">
									<enumeration value="Simple co-current"/> <!-- Simple equicorriente -->
									<enumeration value="Simple counter-current"/> <!-- Simple contracorriente -->
									<enumeration value="Shell and tube"/>
										<!-- Carcasa y tubos -->
										<!-- En el caso de elegir "Shell and tube" debe pedir un campo nuevo llamado "Number of passes through shell" (número de pasos por carcasa, ver más abajo)-->
									<enumeration value="Crossed flows: simple pass, fluids not mixed"/> 
									<enumeration value="Crossed flows: simple pass, fluid 1 mixed, fluid 2 not mixed"/>
									<enumeration value="Crossed flows: simple pass, fluid 1 not mixed, fluid 2 mixed"/>
									<enumeration value="Crossed flows: simple pass, both fluids mixed"/>
								</restriction>
							</simpleType>
						</attribute>
						<attribute name="Number of passes through shell" type="unsignedInt" use="required"></attribute>
							<!-- Número de pasos por carcasa -->
							<!-- Por defecto: 1 -->
							<!-- Este campo sólo debe aparecer si se ha elegido "Shell and tube" en "Heat exchanger type" -->
						
						<!-- OPCIÓN TABLA -->
						<sequence>
							<element maxOccurs="1" minOccurs="0" name="Efficiency table">
								<complexType>
									<!-- Columnas de una tabla que contiene los valores de la eficiencia en función de los valores de NTU y Cr (adimensionales) -->
									<!-- El resultado en el fichero XML podría ser algo así:
									<Efficiency_table>
										<EffTableHtx_Point NTU="0" Cr="0" Efficiency="1" />
										<EffTableHtx_Point NTU="9999" Cr="1" Efficiency="1" />
										-->
									<!-- Por defecto: intercambiador ideal, eficiencia = 1 -->
									<!-- Estaría bien que, como alternativa a rellenar la tabla manualmente, se permitiera la subida o copia de un fichero -->
								   <attribute name="NTU" type="xsd:double"></attribute> <!-- 2 valores por defecto: 0 y 9999 -->
								   <attribute name="Cr" type="xsd:double"></attribute> <!-- 2 valores por defecto: 0 y 1 -->
								   <attribute name="Efficiency" type="xsd:double"></attribute> <!-- 2 valores por defecto: 1 y 1 -->
								</complexType>
							</element>
						</sequence>
					</complexType>
				</element>
            </sequence>
        </complexType>
    </element>
	
</schema>