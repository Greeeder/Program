<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="http://www.openwam.com/VEMOD"
	elementFormDefault="qualified" xmlns="http://www.w3.org/2001/XMLSchema"
	xmlns:ow="http://www.openwam.com/VEMOD">

	<element name="OpenWAM">
		<complexType>
			<sequence>
				<element ref="ow:GeneralData" maxOccurs="1"
					minOccurs="1">
				</element>
				<element ref="ow:EngineBlock" maxOccurs="1"
					minOccurs="0">
				</element>
				<element ref="ow:BlockOfPipes" maxOccurs="1"
					minOccurs="1">
				</element>

				<element ref="ow:BlockOfTurbocharger" maxOccurs="1"
					minOccurs="0">
				</element>
				<element ref="ow:BlockOfPlenums" minOccurs="0" maxOccurs="1"></element>
			</sequence>
		</complexType>
	</element>


	<element name="NewEngineBlock" type="ow:EngineBlockType"></element>

	<complexType name="EngineBlockType">
		<sequence>
			<element name="Cylinder" type="ow:CylinderType"
				minOccurs="1" maxOccurs="unbounded">
			</element>
			<element name="Combustion" type="ow:CombustionType"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="Eng_Geometry" type="ow:Eng_GeometryType"
				maxOccurs="1" minOccurs="1">
			</element>

			<element ref="ow:Actuator" maxOccurs="unbounded"
				minOccurs="0">
			</element>
			<element ref="ow:GasProperties" minOccurs="1"
				maxOccurs="1">
			</element>
		</sequence>
		<attribute name="Type">
			<simpleType>
				<restriction base="string">
					<enumeration value="4Strokes"></enumeration>
					<enumeration value="2Strokes"></enumeration>
				</restriction>
			</simpleType>
		</attribute>
		<attribute name="Speed" type="double" use="required"></attribute>
	</complexType>

	<complexType name="CylinderType">
		<sequence>
			<element name="Cyl_AvgOutput" type="ow:Cyl_AvgOutputType"></element>
			<element name="Cyl_InsOutput" type="ow:CylInsOutputType"></element>
		</sequence>
		<attribute name="Cyl_ID" type="ID" use="required">
			<annotation>
				<documentation>
					Use the following mask: cyl:XX where XX is the
					number of the cylinder
				</documentation>
			</annotation>
		</attribute>
		<attribute name="Order" type="int" use="required"></attribute>
	</complexType>

	<complexType name="CombustionType">
		<choice>
			<element name="MultiWiebe" type="ow:MultiWiebeType"></element>
			<element name="HRL" type="ow:HRLType"></element>
			<element ref="ow:Wiebe"></element>
			<element name="CombustionModel"
				type="ow:CombustionModelType">
			</element>
		</choice>
	</complexType>

	<complexType name="MultiWiebeType">
		<sequence>
			<element name="SingleWiebe" type="ow:SingleWiebeType"
				minOccurs="1" maxOccurs="unbounded">
			</element>
		</sequence>
	</complexType>

	<element name="Wiebe" type="ow:WiebeType"></element>

	<complexType name="WiebeType">
		<attribute name="Duration" type="double"></attribute>
		<attribute name="SOC" type="double"></attribute>
		<attribute name="m" type="double"></attribute>
		<attribute name="C" type="double"></attribute>
	</complexType>

	<complexType name="HRLType">
		<sequence>
			<element name="HRLrow" type="ow:HRLrowType" minOccurs="1"
				maxOccurs="unbounded">
			</element>
		</sequence>
		<attribute name="SOC" type="double" use="required"></attribute>
	</complexType>

	<complexType name="CombustionModelType"></complexType>

	<complexType name="HRLrowType">
		<attribute name="Angle" type="double" use="required"></attribute>
		<attribute name="HRLvalue" type="double" use="required"></attribute>
	</complexType>

	<complexType name="SingleWiebeType">
		<sequence>
			<element ref="ow:Wiebe"></element>
		</sequence>
		<attribute name="Beta" type="double" use="required"></attribute>
	</complexType>

	<element name="BlockOfPipes" type="ow:BlockOfPipesType"></element>

	<complexType name="BlockOfPipesType">
		<sequence>
			<element name="Bop_NewPipe" type="ow:Bop_NewPipesType" maxOccurs="unbounded" minOccurs="0"></element>
		</sequence>
	</complexType>

	<element name="GeneralData" type="ow:GeneralDataType"></element>

	<complexType name="GeneralDataType">
		<sequence>
			<element name="SpeciesModel" type="ow:SpeciesModelType"
				minOccurs="1" maxOccurs="1">
			</element>
			<element name="Materials" type="ow:MaterialsType"
				minOccurs="1" maxOccurs="1">
			</element>
		</sequence>
	</complexType>

	<complexType name="SpeciesModelType">
		<sequence>
			<element name="Specie" type="ow:SpecieType" minOccurs="1"
				maxOccurs="unbounded">
			</element>
		</sequence>
		<attribute name="FuelType">
			<simpleType>
				<restriction base="string">
					<enumeration value="GASOLINE"></enumeration>
					<enumeration value="GASOIL"></enumeration>
					<enumeration value="BLEND"></enumeration>
				</restriction>
			</simpleType>
		</attribute>
	</complexType>

	<complexType name="MaterialsType">
		<sequence>
			<element name="Material" type="ow:MaterialType"
				maxOccurs="unbounded" minOccurs="1">
			</element>
		</sequence>
	</complexType>

	<complexType name="SpecieType">
		<sequence>
			<element name="Component" type="ow:ComponentType"
				minOccurs="0" maxOccurs="unbounded">
			</element>
		</sequence>
		<attribute name="Name">
			<simpleType>
				<restriction base="ID">
					<enumeration value="AIR"></enumeration>
					<enumeration value="FUEL"></enumeration>
					<enumeration value="GAS"></enumeration>
					<enumeration value="O2"></enumeration>
					<enumeration value="N2"></enumeration>
					<enumeration value="CO2"></enumeration>
					<enumeration value="H2Ov"></enumeration>
					<enumeration value="CO"></enumeration>
					<enumeration value="C"></enumeration>
					<enumeration value="NO"></enumeration>
					<enumeration value="NO2"></enumeration>
				</restriction>
			</simpleType>
		</attribute>
		<attribute name="MolecularWeigth" type="double"></attribute>
		<attribute name="O2_Mol_Fraction" type="double"></attribute>
		<attribute name="Density" type="double"></attribute>
		<attribute name="LowerCalorificValue" type="double"></attribute>
		<attribute name="CAHFL" type="double"></attribute>
		<attribute name="CBHFL" type="double"></attribute>
		<attribute name="O_Content" type="double"></attribute>
		<attribute name="Relation_HC" type="double"></attribute>
	</complexType>

	<complexType name="ComponentType">
		<attribute name="Name">
			<simpleType>
				<restriction base="string">
					<enumeration value="GASOLINA"></enumeration>
					<enumeration value="GASOIL"></enumeration>
				</restriction>
			</simpleType>
		</attribute>
		<attribute name="MolecularWeight" type="double"></attribute>
		<attribute name="Density" type="double"></attribute>
		<attribute name="LowerCalorificValue" type="double"></attribute>
		<attribute name="CAHFL" type="double"></attribute>
		<attribute name="CBHFL" type="double"></attribute>
		<attribute name="O_Content" type="double"></attribute>
		<attribute name="Relation_HC" type="double"></attribute>
		<attribute name="Y_Fraction" type="double"></attribute>
	</complexType>

	<complexType name="MaterialType">
		<sequence>
			<element name="Conductivity" type="ow:ConductivityType"
				maxOccurs="1" minOccurs="0">
			</element>
			<element name="HeatCapacity" type="ow:HeatCapacityType"
				maxOccurs="1" minOccurs="0">
			</element>
			<element name="Density" type="ow:DensityType" maxOccurs="1"
				minOccurs="0">
			</element>
		</sequence>
		<attribute name="Name" type="ID"></attribute>
	</complexType>

	<complexType name="ConductivityType">
		<sequence>
			<element name="ConductivityCoef"
				type="ow:ConductivityCoefType" maxOccurs="unbounded"
				minOccurs="1">
			</element>
		</sequence>
	</complexType>

	<complexType name="HeatCapacityType">
		<sequence>
			<element name="HeatCapacityCoef"
				type="ow:HeatCapacityCoefType" maxOccurs="unbounded"
				minOccurs="1">
			</element>
		</sequence>
	</complexType>

	<complexType name="DensityType">
		<sequence>
			<element name="DensityCoef" type="ow:DensityCoefType"
				maxOccurs="unbounded" minOccurs="1">
			</element>
		</sequence>
	</complexType>

	<complexType name="ConductivityCoefType">
		<sequence></sequence>
		<attribute name="ID" type="int"></attribute>
		<attribute name="Value" type="double"></attribute>
	</complexType>

	<complexType name="HeatCapacityCoefType">
		<attribute name="ID" type="int"></attribute>
		<attribute name="Value" type="double"></attribute>
	</complexType>

	<complexType name="DensityCoefType">
		<attribute name="ID" type="int"></attribute>
		<attribute name="Value" type="double"></attribute>
	</complexType>

	<element name="BlockOfTurbocharger"
		type="ow:BlockOfTurbochargersType">
	</element>

	<complexType name="BlockOfTurbochargersType">
		<sequence>
			<element name="Bot_Turbocharger"
				type="ow:Bot_TurbochargerType" maxOccurs="unbounded"
				minOccurs="1">
			</element>
		</sequence>
	</complexType>

	<complexType name="Bot_TurbochargerType">
		<sequence>
			<element name="Tch_Compressor" type="ow:Tch_CompressorType"
				maxOccurs="unbounded" minOccurs="1">
			</element>
			<element name="Tch_Turbine" type="ow:Tch_TurbineType"
				maxOccurs="unbounded" minOccurs="1">
			</element>
			<element name="Tch_HTML" type="ow:Tch_HTMLType"
				maxOccurs="1" minOccurs="0">
			</element>
			<element ref="ow:Actuator" maxOccurs="unbounded"
				minOccurs="0">
			</element>
			<element name="Tch_AvgOutput" type="ow:Tch_AvgOutputType"
				maxOccurs="1" minOccurs="0">
			</element>
			<element name="Tch_InsOutput" type="ow:Tch_InsOutputType"
				maxOccurs="1" minOccurs="0">
			</element>
		</sequence>
		<attribute name="Turbocharger_ID" type="string"
			use="required">
		</attribute>
		<attribute name="Speed" type="double" use="required"></attribute>
		<attribute name="FixedSpeed" type="boolean" use="required"></attribute>
		<attribute name="Inertia" type="double" use="optional"></attribute>
	</complexType>

	<complexType name="Tch_CompressorType">
		<attribute name="Compressor_ID" type="int" use="required"></attribute>
	</complexType>

	<complexType name="Tch_TurbineType">
		<attribute name="Turbine_ID" type="int" use="required"></attribute>
	</complexType>

	<complexType name="Tch_HTMLType">
		<sequence>
			<element name="Htm_Bearings" type="ow:Htm_BearingsType"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="Htm_Fluids" type="ow:Htm_FluidsType"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="Htm_MechLosses" type="ow:Htm_MechLossesType"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="Htm_Geometry" type="ow:Htm_GeometryType"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="Htm_HeatTransfer"
				type="ow:Htm_HeatTransferType" maxOccurs="1" minOccurs="1">
			</element>

		</sequence>
	</complexType>


	<complexType name="Htm_BearingsType">
		<attribute name="ShaftDiameter" type="double" use="required"></attribute>
		<attribute name="JournalBearingLength" type="double"
			use="required">
			<annotation>
				<documentation>
					This is the total length of the journal bearings
				</documentation>
			</annotation>
		</attribute>
		<attribute name="Clearence" type="double" use="required"></attribute>
		<attribute name="ThrustBearingMinR" type="double"
			use="required">
		</attribute>
		<attribute name="ThrustBearingMaxR" type="double"
			use="required">
		</attribute>
	</complexType>

	<complexType name="Htm_FluidsType">
		<sequence>
			<element name="Flu_Oil" type="ow:Flu_OilType" maxOccurs="1"
				minOccurs="1">
			</element>
			<element name="Flu_Water" type="ow:Flu_WaterType"
				maxOccurs="1" minOccurs="0">
			</element>
		</sequence>
	</complexType>

	<complexType name="Htm_MechLossesType">
		<attribute name="JournalB_K" type="double" use="required"></attribute>
		<attribute name="K_m" type="double" use="required"></attribute>
		<attribute name="K_tb" type="double" use="required"></attribute>
		<attribute name="CAC" type="double" use="required"></attribute>
		<attribute name="CAT" type="double" use="required"></attribute>
	</complexType>

	<complexType name="Htm_GeometryType">
		<attribute name="CompWheelArea" type="double" use="required"></attribute>
		<attribute name="TurbWheelArea" type="double" use="required"></attribute>
		<attribute name="TurbExtDiameter" type="double"
			use="required">
		</attribute>
		<attribute name="TurbExtLength" type="double" use="required"></attribute>
		<attribute name="CompExtDiamter" type="double" use="required"></attribute>
		<attribute name="CompExtLength" type="double" use="required"></attribute>
		<attribute name="HousExtDiameter" type="double"
			use="required">
		</attribute>
		<attribute name="housExtLength" type="double" use="required"></attribute>
	</complexType>

	<complexType name="Htm_HeatTransferType">
		<sequence>
			<element name="Htr_Settings" type="ow:Htr_SettingsType"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="Htr_Conduction" type="ow:Htr_ConductionType"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="Htr_Capacity" type="ow:Htr_CapacityType"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="Htr_Convection" type="ow:Htr_ConvectionType"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="Htr_Radiation" type="ow:Htr_RadiationType"
				maxOccurs="1" minOccurs="0">
			</element>
			<element name="Htr_ConvectionExt"
				type="ow:Htr_ConvectionExtType" maxOccurs="1" minOccurs="0">
			</element>
			<element name="Htr_Temperatures"
				type="ow:Ttr_TemperaturesType" maxOccurs="1" minOccurs="0">
			</element>
		</sequence>
	</complexType>

	<complexType name="Flu_OilType">
		<attribute name="PortDiameter" type="double" use="required"></attribute>
		<attribute name="MassFlow" type="double" use="required"></attribute>
		<attribute name="Temperature" type="double" use="required"></attribute>
		<attribute name="Pressure" type="double" use="required"></attribute>
		<attribute name="VoeguelK1" type="double" use="required"></attribute>
		<attribute name="VoeguelK2" type="double" use="required"></attribute>
		<attribute name="VoeguelK3" type="double" use="required"></attribute>
	</complexType>

	<complexType name="Flu_WaterType">
		<attribute name="PortDiameter" type="double" use="required"></attribute>
		<attribute name="MassFlow" type="double" use="required"></attribute>
		<attribute name="Temperature" type="double" use="required"></attribute>
	</complexType>

	<complexType name="Htr_SettingsType">
		<attribute name="TimeStep" type="double" use="optional"></attribute>
		<attribute name="MaxTime" type="double" use="optional"></attribute>
		<attribute name="FastConvergence" type="boolean"
			use="required">
		</attribute>
		<attribute name="WaterCooled" type="boolean"></attribute>
	</complexType>

	<complexType name="Htr_ConductionType">
		<attribute name="T_H1" type="double" use="required"></attribute>
		<attribute name="H1_H2" type="double"></attribute>
		<attribute name="H2_H3" type="double" use="required"></attribute>
		<attribute name="H3_C" type="double"></attribute>
	</complexType>

	<complexType name="Htr_CapacityType">
		<attribute name="T" type="double" use="required"></attribute>
		<attribute name="H1" type="double" use="required"></attribute>
		<attribute name="H2" type="double" use="required"></attribute>
		<attribute name="H3" type="double" use="required"></attribute>
		<attribute name="C" type="double" use="required"></attribute>
	</complexType>

	<complexType name="Htr_ConvectionType">
		<sequence>
			<element name="Cnv_Gas_T" type="ow:Cnv_Gas_TType"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="Cnv_Wat_H2" type="ow:Cnv_Wat_H2Type"
				maxOccurs="1" minOccurs="0">
			</element>
			<element name="Cnv_Oil_H1" type="ow:Cnv_Oil_H1Type"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="Cnv_Oil_H2_C" type="ow:Cnv_Oil_H2_CType"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="Cnv_Oil_H2_H" type="ow:Cnv_Oil_H2_HType"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="Cnv_Air_C_C" type="ow:Cnv_Air_C_CType"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="Cnv_Air_C_H" type="ow:Cnv_Air_C_HType"
				maxOccurs="1" minOccurs="1">
			</element>
		</sequence>
	</complexType>

	<complexType name="Htr_RadiationType">
		<attribute name="FitCoefficient" type="double" use="required"></attribute>
		<attribute name="TurbineEmissivity" type="double"
			use="required">
		</attribute>
		<attribute name="HousingEmissivity" type="double"
			use="required">
		</attribute>
		<attribute name="CompressorEmissivity" type="double"
			use="required">
		</attribute>
		<attribute name="ShieldEmissivity" type="double"
			use="optional">
		</attribute>
	</complexType>

	<complexType name="Htr_ConvectionExtType">
		<choice>
			<element name="Ecv_HeatCoefficients"
				type="ow:Ecv_HeatCoefficientsType" maxOccurs="1" minOccurs="0">
			</element>
			<element name="Ecv_Velocities" type="ow:Ecv_VelocitiesType"
				maxOccurs="1" minOccurs="0">
			</element>
		</choice>
		<attribute name="FitCoefficient" type="double" use="required"></attribute>
		<attribute name="ExtVelocity" type="double" use="optional"></attribute>
	</complexType>

	<complexType name="Cnv_Gas_TType">
		<attribute name="k1" type="double" use="required"></attribute>
		<attribute name="k2" type="double" use="required"></attribute>
		<attribute name="k3" type="double" use="required"></attribute>
		<attribute name="k4" type="double" use="required"></attribute>
	</complexType>

	<complexType name="Cnv_Wat_H2Type">
		<attribute name="k1" type="double" use="required"></attribute>
		<attribute name="k2" type="double" use="required"></attribute>
		<attribute name="k3" type="double" use="required"></attribute>
	</complexType>

	<complexType name="Cnv_Oil_H1Type">
		<attribute name="k1" type="double" use="required"></attribute>
		<attribute name="k2" type="double" use="required"></attribute>
		<attribute name="k3" type="double" use="required"></attribute>
		<attribute name="k4" type="double" use="required"></attribute>
		<attribute name="k5" type="double" use="required"></attribute>
	</complexType>

	<complexType name="Cnv_Oil_H2_CType">
		<attribute name="k1" type="double" use="required"></attribute>
		<attribute name="k2" type="double" use="required"></attribute>
		<attribute name="k3" type="double" use="required"></attribute>
		<attribute name="k4" type="double" use="required"></attribute>
	</complexType>

	<complexType name="Cnv_Oil_H2_HType">
		<attribute name="k1" type="double" use="required"></attribute>
		<attribute name="k2" type="double" use="required"></attribute>
		<attribute name="k3" type="double" use="required"></attribute>
		<attribute name="k4" type="double" use="required"></attribute>
	</complexType>

	<complexType name="Cnv_Air_C_CType">
		<attribute name="k1" type="double" use="required"></attribute>
		<attribute name="k2" type="double" use="required"></attribute>
		<attribute name="k3" type="double" use="required"></attribute>
	</complexType>

	<complexType name="Cnv_Air_C_HType">
		<attribute name="k1" type="double" use="required"></attribute>
		<attribute name="k2" type="double" use="required"></attribute>
		<attribute name="k3" type="double" use="required"></attribute>
	</complexType>

	<complexType name="Ttr_TemperaturesType">
		<attribute name="T" type="double" use="required"></attribute>
		<attribute name="H1" type="double" use="required"></attribute>
		<attribute name="H2" type="double" use="required"></attribute>
		<attribute name="H3" type="double" use="required"></attribute>
		<attribute name="C" type="double" use="required"></attribute>
	</complexType>

	<complexType name="Ecv_HeatCoefficientsType">
		<sequence></sequence>
		<attribute name="Turbine" type="double" use="required"></attribute>
		<attribute name="Housing" type="double" use="required"></attribute>
		<attribute name="Compressor" type="double" use="required"></attribute>
	</complexType>

	<complexType name="Ecv_VelocitiesType">
		<attribute name="Turbine" type="double" use="required"></attribute>
		<attribute name="Housing" type="double" use="required"></attribute>
		<attribute name="Compressor" type="double" use="required"></attribute>
	</complexType>

	<element name="Actuator" type="ow:ActuatorType"></element>

	<complexType name="ActuatorType">
		<attribute name="CtrlID" type="IDREF" use="required"></attribute>
		<attribute name="Parameter" type="string" use="required"></attribute>
	</complexType>

	<complexType name="Tch_AvgOutputType">
		<attribute name="Parameter" use="required">
			<simpleType>
				<restriction base="string">
					<enumeration value="Speed"></enumeration>
				</restriction>
			</simpleType>
		</attribute>
	</complexType>

	<complexType name="Tch_InsOutputType">
		<attribute name="Paramter">
			<simpleType>
				<restriction base="string">
					<enumeration value="Speed"></enumeration>
					<enumeration value="MechanicalLosses"></enumeration>
					<enumeration value="MechanicalEfficiency"></enumeration>
					<enumeration value="NodeTemperature"></enumeration>
					<enumeration value="HeatFlow"></enumeration>
				</restriction>
			</simpleType>
		</attribute>
	</complexType>

	<complexType name="Eng_GeometryType">
		<attribute name="Diameter" type="double" use="required"></attribute>
		<attribute name="CrankRadius" type="double" use="required"></attribute>
		<attribute name="ConnectingRodLength" type="double"
			use="required">
		</attribute>
		<attribute name="Eccentricity" type="double" use="required"></attribute>
		<attribute name="CompressionRatio" type="double"
			use="required">
		</attribute>
		<attribute name="PressureCoefficient" type="double"
			use="required">
		</attribute>
		<attribute name="InertiaCoefficient" type="double"
			use="required">
		</attribute>
	</complexType>

	<complexType name="Cyl_AvgOutputType">
		<attribute name="Parameter">
